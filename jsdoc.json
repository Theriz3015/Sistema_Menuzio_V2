{
    "source": {
        "include": ["./server.js", "./routes", "./models", "./middlewares"], 
        "includePattern": ".js$",
        "excludePattern": "(node_modules|docs_jsdoc)"
    },
    "plugins": [],
    "opts": {
        "encoding": "utf8",
        "destination": "./docs_jsdoc/",
        "recurse": true
    },
    "templates": {
        "cleverLinks": true,
        "monospaceLinks": true
    }
}